{
  "name": "rpd-purs",
  "version": "0.1.0",
  "private": true,
  "description": "Reactive Patch Development with PureScript",
  "license": "MIT",
  "repository": "shamansir/rpd-purs",
  "author": {
    "name": "Ulric Wilfred",
    "email": "shaman.sir@gmail.com"
  },
  "files": [],
  "scripts": {
    "webpack": "DEBUG=purs-loader* DEBUG_DEPTH=100 webpack --progress --bail",
    "webpack:watch": "PATH=$PATH:../releases/psc-package DEBUG=purs-loader* DEBUG_DEPTH=100 webpack --progress --display-error-details --display verbose --watch",
    "webpack:server": "webpack-dev-server --progress --inline --hot",
    "webpack:server:debug": "DEBUG=purs-loader* DEBUG_DEPTH=100 webpack-dev-server --progress --inline --hot",
    "purs:compile": "purs compile 'bower_components/purescript-*/src/**/*.purs' 'src/**/*.purs'",
    "purs:compile-all": "purs compile 'bower_components/purescript-*/src/**/*.purs' 'src/**/*.purs' 'test/**/*.purs' 'example/**/*.purs'",
    "purs:bundle": "purs bundle output/**/*.js",
    "purs:build": "pulp build",
    "purs:server": "pulp --watch server -I ./example --main Example.HalogenVDom",
    "purs:repl": "purs repl 'bower_components/purescript-*/src/**/*.purs' 'src/**/*.purs'",
    "purs:mod-repl": "purs repl 'bower_components/purescript-*/src/**/*.purs'",
    "purs:test": "pulp test --no-check-main",
    "start": "npm run purs:server"
  },
  "dependencies": {
    "lodash.difference": "^4.5.0",
    "purescript": "^0.12.0",
    "pulp": "^12.3.0",
    "purescript-psa": "^0.7.3",
    "purs-loader": "^3.2.0",
    "webpack": "^4.16.5",
    "webpack-dev-server": "^3.1.5",
    "wrappy": "^1.0.2"
  }
}
